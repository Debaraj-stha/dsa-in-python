    "def sum_triangle(arr):\n",
    "    if(len(arr)<1):\n",
    "        return\n",
    "    temp=[0]*(len(arr)-1)\n",
    "    for i in range(len(arr)-1):\n",
    "        temp[i]=arr[i]+arr[i+1]\n",
    "    sum_triangle(temp)\n",
    "    print(arr)\n",
    "\n",
    "\n",
    "arr=[1,2,3,4,5]\n",
    "sum_triangle(arr)\n"
   ]
  }

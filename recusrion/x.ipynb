    "def x(start,out,n):\n",
    "    if n==0:\n",
    "        print(\" \".join(map(str,out)))\n",
    "        return\n",
    "    for i in range(start,10):\n",
    "        out.append(i)\n",
    "        x(i+1, out, n-1)\n",
    "        out.pop()\n",
    "n = 2 \n",
    "x(0,[],n)"

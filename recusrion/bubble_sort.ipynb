   "source": [
    "def bubble_sort(arr,n):\n",
    "    if n==0:\n",
    "        return arr\n",
    "    for i in range(n-1):\n",
    "        if arr[i]>arr[i+1]:\n",
    "            arr[i],arr[i+1]=arr[i+1],arr[i]\n",
    "    return bubble_sort(arr,n-1)\n",
    "arr=[8,9,4,1,7,6,10,3,2,11,34]\n",
    "n=len(arr)\n",
    "print(bubble_sort(arr,n))"
   ]
  }

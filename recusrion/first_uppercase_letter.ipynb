   "source": [
    "def first_uppercase(string):\n",
    "    if len(string) == 0:\n",
    "        return -1\n",
    "    char=string[0]\n",
    "    if char.isupper():\n",
    "        return char\n",
    "    else:\n",
    "        return first_uppercase(string[1:])\n",
    "\n",
    "first_uppercase(\"hello world\")\n"
   ]
  }

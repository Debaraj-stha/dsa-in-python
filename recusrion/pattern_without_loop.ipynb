    "def pattern(n,m,flag):\n",
    "    print(m,end=\" \")\n",
    "    if  flag ==False and m==n:\n",
    "        return\n",
    "    if flag:\n",
    "        if m-5>0:\n",
    "            pattern(n,m-5,True)\n",
    "        else:\n",
    "            pattern(n,m-5,False)\n",
    "    else:\n",
    "        pattern(n,m+5,False)\n",
    "\n",
    "n=10\n",
    "pattern(n,n,True)\n"
   ]

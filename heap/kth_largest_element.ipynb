    "import heapq\n",
    "def kth_largest_element(k,arr):\n",
    "    min_heap = []\n",
    "    for num in arr:\n",
    "        heapq.heappush(min_heap, num)\n",
    "        if len(min_heap) > k:\n",
    "\n",
    "            heapq.heappop(min_heap)\n",
    "    return heapq.heappop(min_heap)\n",
    "arr=[7,4,3,10,20,15]\n",
    "k=3\n",
    "kth_largest_element(k,arr)"
   ]
  }

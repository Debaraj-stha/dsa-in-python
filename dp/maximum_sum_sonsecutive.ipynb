    "def maximum_sum(arr):\n",
    "    n=len(arr)\n",
    "    dp=arr[:]  # Initialize dp with the same size as arr\n",
    "    for i in range(n):\n",
    "        for j in range(i):\n",
    "            if arr[i]>arr[j]:\n",
    "                dp[i]=max(dp[i],dp[j]+arr[i])\n",
    "    return max(dp)\n",
    "\n",
    "arr=[2,4,1,5,3,6]\n",
    "print(maximum_sum(arr))"
   ]
  }
